<!DOCTYPE html>
<html>
  <head>
    <title>Playoff Predictions Challenge</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'vue-loader' %>
  </head>

  <body>
    <%= render 'nav_bar', current_round: current_round %>
    <div class="placeholder"></div>
    <%= yield %>
    <%= render 'footer' %>
  </body>
</html>
