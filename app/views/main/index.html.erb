<h3>Scoreboard</h3>
<%= link_to "All Series", series_index_path %>
<%= link_to "This Round", predictions_path %>
<%= link_to "Example", new_prediction_path({id: 2}) %>
<table>
  <thead>
    <tr>
      <th>Rank</th>
      <th>Name</th>
      <th>Points</th>
      <th>Correct Predictions</th>
      <th>Lower-Seed Wins Correct</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.rank %></td>
      <td><%= user.given_name %></td>
      <td><%= user.score %></td>
      <td><%= user.correct_predictions %></td>
      <td><%= user.correct_lower_seed_picks %></td>
    </tr>
    <% end %>
  </tbody>
</table>